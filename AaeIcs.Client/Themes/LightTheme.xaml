    <Style x:Key="CustomChromelessWindowStyle"
           TargetType="{x:Type syncfusion:ChromelessWindow}"
           BasedOn="{StaticResource SyncfusionChromelessWindowStyle}">
        <Setter Property="CornerRadius" Value="10"/>
        <Setter Property="ResizeBorderThickness" Value="10"/>
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="Background" Value="#F3F5F7"/> <Style.Triggers>
            <Trigger Property="WindowState" Value="Maximized">
                <Setter Property="CornerRadius" Value="0"/>
                <Setter Property="ResizeBorderThickness" Value="0"/>
                <Setter Property="BorderThickness" Value="0"/>
            </Trigger>
            <Trigger Property="WindowState" Value="Normal">
                <Setter Property="CornerRadius" Value="10"/>
                <Setter Property="ResizeBorderThickness" Value="10"/>
                <Setter Property="BorderThickness" Value="0"/>
                <Setter Property="BorderBrush" Value="Transparent"/>
            </Trigger>
        </Style.Triggers>
    </Style>

